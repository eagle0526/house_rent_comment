<h2>使用者列表</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :nickname %><br />
    <%= f.text_field :nickname, autofocus: true, autocomplete: "email", class: "form-field" %>
  </div>


  <div class="mt-4">
    <%= f.submit "修改暱稱", class: "btn-primary" %>
  </div>
<% end %>


